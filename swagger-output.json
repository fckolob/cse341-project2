{
  "swagger": "2.0",
  "info": {
    "title": "Pet Store API",
    "description": "API documentation for the Pet Store project",
    "version": "1.0.0"
  },
  "host": "cse341-project2-wok4.onrender.com",
  "basePath": "/",
  "schemes": [
    "https"
  ],
  "paths": {
    "/": {
      "get": {
        "description": "API root endpoint. Returns a welcome message.",
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/github/callback": {
      "get": {
        "description": "GitHub OAuth callback endpoint.",
        "responses": {
          "302": {
            "description": "Redirect after successful authentication"
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/login": {
      "get": {
        "description": "Initiates GitHub login.",
        "responses": {
          "302": {
            "description": "Redirect to GitHub login"
          }
        }
      }
    },
    "/logout": {
      "get": {
        "description": "Logs out the current user.",
        "responses": {
          "200": {
            "description": "Successfully logged out"
          }
        }
      }
    },
    "/pets/": {
      "get": {
        "description": "Returns all pets in the database.",
        "responses": {
          "204": {
            "description": "OK"
          }, "200":{
            "description" : "Ok"
          }, "500": { "description": "Failed to fetch pets"}
        }
      },
      "post": {
        "description": "Creates a new pet.",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "name": { "type": "string", "example": "duke" },
                "age": { "type": "number", "example": 3 },
                "sex": { "type": "string", "example": "male" },
                "hairColor": { "type": "string", "example": "black" },
                "weight": { "type": "number", "example": 3 },
                "vaccines": { "type": "string", "example": "updated" },
                "routineControll": { "type": "string", "example": "updated" }
              }
            }
          }
        ],
        "responses": {
          "204": { "description": "Pet created successfully" },
          "422": { "description": "Validation Error" },
          "481": { "description": "Authentication Error" }, "200":{
            "description" : "Pet created successfully"
          }, "500": { "description": "Some error ocurred while creating the pet"}
        }
      }
    },
    "/pets/{id}": {
      "get": {
        "description": "Returns a single pet by ID.",
        "parameters": [
          { "name": "id", "in": "path", "required": true, "type": "string", "description": "Pet ID" }
        ],
        "responses": {
          "204": { "description": "OK" }, "200":{
            "description" : "Ok"
          },
          "404": { "description": "Pet not found" }
        }, "500": { "description": "Failed to fetch pet"}
      },
      "put": {
        "description": "Updates a pet by ID.",
        "parameters": [
          { "name": "id", "in": "path", "required": true, "type": "string", "description": "Pet ID" },
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "name": { "type": "string", "example": "duke" },
                "age": { "type": "number", "example": 3 },
                "sex": { "type": "string", "example": "male" },
                "hairColor": { "type": "string", "example": "black" },
                "weight": { "type": "number", "example": 3 },
                "vaccines": { "type": "string", "example": "updated" },
                "routineControll": { "type": "string", "example": "updated" }
              }
            }
          }
        ],
        "responses": {
          "204": { "description": "Pet updated successfully" }, "200":{
            "description" : "Pet updated successfully"
          },
          "422": { "description": "Unprocessable Entity" },
          "404": { "description": "Pet not found" },
          "481": { "description": "Authentication Error" }, "500": { "description": "Some error ocurred while updating pet"}
        }
      },
      "delete": {
        "description": "Deletes a pet by ID.",
        "parameters": [
          { "name": "id", "in": "path", "required": true, "type": "string", "description": "Pet ID" }
        ],
        "responses": {
          "204": { "description": "Pet deleted successfully" }, "200":{
            "description" : "Pet deleted successfully"
          },
          "404": { "description": "Pet not found" },
          "481": { "description": "Authentication Error" }, "500": { "description": "Some error ocurred while updating the pet"}
        }
      }
    },
    "/products/": {
      "get": {
        "description": "Returns all animal products.",
        "responses": {
          "204": { "description": "OK" },
          "200": { "description": "OK"}, "500": { "description": "Failed to fetch products"}
        }
      },
      "post": {
        "description": "Creates a new animal product.",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "name": { "type": "string", "example": "dog collar" },
                "brand": { "type": "string", "example": "Fancy Cat" },
                "description": { "type": "string", "example": "A very elegant cat collar." },
                "price": { "type": "number", "example": 20.25 },
                "color": { "type": "string", "example": "golden" },
                "inStock": { "type": "string", "example": "yes" },
                "size": { "type": "string", "example": "medium" }
              }
            }
          }
        ],
        "responses": {
          "204": { "description": "Product created successfully" },
          "200":{"description": "Product created succesfully"},
          "422": { "description": "Unprocessable Entity" },
          "481": { "description": "Authentication Error" }, "500": { "description": "Some error ocurred while creating product"}
        }
      }
    },
    "/products/{id}": {
      "get": {
        "description": "Returns a single product by ID.",
        "parameters": [
          { "name": "id", "in": "path", "required": true, "type": "string", "description": "Product ID" }
        ],
        "responses": {
          "204": { "description": "OK" },
          "404": { "description": "Product not found" }, "500": { "description": "Failed to fetch product"}
        }
      },
      "put": {
        "description": "Updates a product by ID.",
        "parameters": [
          { "name": "id", "in": "path", "required": true, "type": "string", "description": "Product ID" },
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "name": { "type": "string", "example": "dog collar" },
                "brand": { "type": "string", "example": "Fancy Cat" },
                "description": { "type": "string", "example": "A very elegant cat collar." },
                "price": { "type": "number", "example": 20.25 },
                "color": { "type": "string", "example": "golden" },
                "inStock": { "type": "string", "example": "yes" },
                "size": { "type": "string", "example": "medium" }
              }
            }
          }
        ],
        "responses": {
          "204": { "description": "Product updated successfully" },
          "422": { "description": "Unprocessable Entity" },
          "404": { "description": "Product not found" },
          "481": { "description": "Authentication Error" }, "500": { "description": "Some error ocurred while updating product."}
        }
      },
      "delete": {
        "description": "Deletes a product by ID.",
        "parameters": [
          { "name": "id", "in": "path", "required": true, "type": "string", "description": "Product ID" }
        ],
        "responses": {
          "204": { "description": "Product deleted successfully" },
          "404": { "description": "Product not found" },
          "481": { "description": "Authentication Error" }, "500": { "description": "Some error ocurred while deleting product"}
        }
      }
    }
  }
}